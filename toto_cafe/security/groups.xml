<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="module_toto_cafe" model="ir.module.category">
        <field name="name">TOTO CAFE</field>
    </record>

    <record id="cafe_consumer" model="res.groups">
        <field name="name">Consumer</field>
        <field name="category_id" ref="module_toto_cafe"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="cafe_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_toto_cafe"/>
        <field name="implied_ids" eval="[(4, ref('cafe_consumer'))]"/>
    </record>
</odoo>